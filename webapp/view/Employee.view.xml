<mvc:View controllerName="sap.ui.demo.nav.controller.Employee" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form">
	<Page id="employeePage" title="{i18n>EmployeeDetailsOf} {FirstName} {LastName}" showNavButton="true" navButtonPress=".onNavBack">
		<content>
			<Panel id="employeePanel" width="auto">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>formEmployeeIDColon} {EmployeeId}"/>
					</Toolbar>
				</headerToolbar>
				<content>
					<f:SimpleForm>
						<f:content>
							<Label text="{i18n>formFirstName}"/>
							<Text text="{FirstName}"/>
							<Label text="{i18n>formLastName}"/>
							<Text text="{LastName}"/>
						</f:content>
					</f:SimpleForm>
				</content>
			</Panel>
		</content>
	</Page>
</mvc:View>